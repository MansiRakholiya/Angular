<html>
    <head>
        <title>Employee details in formate</title>
    </head>
    <body>
        <h1> Employee Information </h1>
        <table>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Age</th>
                <th>Address</th>
                <th>Salary</th>
                <th>Department</th>
            </tr>
            <tr *ngFor="let emplopyee of employees">
                <td>{{emplopyee.empid}}</td>
                <td>{{emplopyee.empname}}</td>
                <td>{{emplopyee.empage}}</td>
                <td>{{emplopyee.empaddress}}</td>
                <td>{{emplopyee.empsal}}</td>
                <td>{{emplopyee.empdep}}</td>
            </tr>
        </table>
    </body>
</html>



